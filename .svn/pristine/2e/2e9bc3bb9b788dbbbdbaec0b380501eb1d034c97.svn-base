<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="com.cyberway.common.domain.CoreGrade" 
    table="CORE_Grade"
>

    <id
        name="gid"
        type="java.lang.Long"
        column="GID"
    >
        <generator class="native" />
    </id>
 
 <property
        name="gnumber"
        type="java.lang.Long"
        column="GNUMBER"
        length="18"
    />
    <property
        name="gcode"
        type="java.lang.String"
        column="GCODE"
        length="50"
    />
    <property
        name="gname"
        type="java.lang.String"
        column="GNAME"
        length="100"
    />
    <property
        name="gremark"
        type="java.lang.String"
        column="GREMARK"
        length="200"
    />
 
    <property name="siteId" type="java.lang.Long" column="SITEID" length="18" />
  
</class>
</hibernate-mapping>
