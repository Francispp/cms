<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.cyberway.weixin.business.department.domain.Department" table="WX_DEPARTMENT">
        <id name="id" type="java.lang.Long" column="ID">
            <generator class="native" />
        </id>
        <property name="deptId" type="java.lang.Integer" column="DEPT_ID"/>
        <property name="deptName" type="java.lang.String" column="DEPT_NAME"/>
        <property name="parentId" type="java.lang.Integer" column="PARENT_ID"/>
        <property name="writable"  type="java.lang.String" column="WRITABLE" />
    </class>
</hibernate-mapping>