<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping >
    <class name="com.cyberway.common.domain.CoreReport" table="CORE_REPORT">
        <id name="reportid" type="java.lang.Long">
            <generator class="native" />
        </id>
        <property name="reportCode" not-null="true" length="50"/>
		<property name="reportName" length="100"/> 
		<property name="reportSql"  not-null="true" length="1000"/>  
		<property name="choiseField"  length="500"/>
		<property name="selectedField" not-null="true" length="500"/>
		<property name="whereField"  length="500"/>
		<property name="isupdated" />
		<property name="reportStyleType" />
		<property name="tableContent" length="300"/> 
		<property name="theadContent" length="200"/> 
		<property name="tdContent" length="200"/> 
		<property name="remark" length="500"/>
		<property name="pageTop" length="1000"/>
		<property name="pageFoot" length="1000"/>
    </class>
</hibernate-mapping >
