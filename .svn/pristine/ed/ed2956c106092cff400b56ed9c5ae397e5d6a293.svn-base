<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.cyberway.weixin.business.attendance.domain.Attendance" table="WX_ATTENDANCE">
        <id name="id" type="java.lang.Long" column="ID">
            <generator class="native" />
        </id>
            <property name="userId"  type="java.lang.String" column="USER_ID" />
            <property name="signinTime"  type="java.util.Date" column="SIGNIN_TIME" />
            <property name="signOutTime"  type="java.util.Date" column="SIGNOUT_TIME" />
            <property name="latitude"  type="java.lang.Double" column="LATITUDE" />
            <property name="longitude"  type="java.lang.Double" column="LONGITUDE" />
            <property name="precision"  type="java.lang.Double" column="PRECISIONS" />
    </class>
</hibernate-mapping>