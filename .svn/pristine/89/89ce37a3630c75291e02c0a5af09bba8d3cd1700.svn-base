@charset "utf-8";
/* CSS Document */
* {
	margin: 0px;
	padding: 0px;
}
a, area {
	text-decoration: none;
	outline: none;/*ff*/
 hide-focus:expression(this.hideFocus=true);/*ie*/
}
ul {
	padding: 0px;
	list-style: none;
}
.content {
	margin: 0px auto;
	width: 1000px;
	height: 500px;
}
.sideBar {
	float: left;
	padding: 0px;
	width: 250px;
	height: 500px;
	background-color: #e3f2ff;
}
.main {
	margin-left: 250px;
	padding: 15px;
	width: 100%;
	height: 500px;
}
.main h1 {
	padding: 0px 10px;
	height: 40px;
	line-height: 40px;
	font-size: 14px;
	font-weight: normal;
	margin-bottom: 15px;
	color: #FFF;
	background-color: #7eb8e6;
}
.infor {
	font-size: 12px;
	line-height: 180%;
}
.infor p {
	margin-bottom: 15px;
}
.title {
	padding: 0px 10px;
	height: 30px;
	line-height: 30px;
	background-color: #dbe5f1;
}
/*
 *	name	: dataTable
 *	content	: 数据表格
 */

.dataTableBox_header {
	height: 37px;
	background: url(../images/bg_titItem.png) repeat-x;
}
.dataTableBox_header .tabBox {
	display: inline-block;
	margin-top: 6px;
	margin-left: 7px;
	height: 28px;
	line-height: 28px;
	font-size: 12px;
	overflow: hidden;
}
.dataTableBox_header .tabBox_item {
	margin: 0px;
	height: 28px;
	line-height: 28px;
	display: block;
}
.dataTableBox_header .tabBox_item .tab_box1, .dataTableBox_header .tabBox_item .tab_box2, .dataTableBox_header .tabBox_item .tab_box3 {
	background: url(../../images/cms/bg_tab_left.png) no-repeat;
}
.dataTableBox_header .tabBox_item .tab_box1 {
	display: inline-block;
	height: 30px;
	padding-left: 5px;
	padding-top: 0px;
}
.dataTableBox_header .tabBox_item .tab_box2 {
	display: inline-block;
	padding-top: 0px;
	height: 29px;
	padding-right: 23px;
	background-position: right bottom;
}
.dataTableBox_header .tabBox_item .tab_box3 {
	display: inline-block;
	height: 29px;
	min-width: 71px;
	line-height: 25px;
	padding-top: 6px;
	padding-left: 13px;
	background-position: 0 -31px;
	background-repeat: repeat-x;
	text-align: center;
	color: #007fc4;
	font-weight: bold;
}
.menu {
	padding: 10px 20px;
	height: 440px;
	overflow-x: hidden;
	overflow-y: auto;
}
.menu ul li {
	width: 210px;
	height: 25px;
	/* line-height: 25px; */
	font-size: 12px;
	margin-bottom: 5px;
	text-indent: 15px;
	text-overflow: ellipsis;
	white-space: nowrap;
	overflow: hidden;
}
.menu ul li a {
	color: #000;
}
.menu ul li:hover {
	background-color: #95b3d7;
}
.com_ele_pics {
	width: 7px;
	height: 7px;
	background: url(../../images/cms/common_ele_pics.png) no-repeat 0px 0px;
	display: inline-block;
*display:inline;
*zoom:1;
	vertical-align: middle;
	margin-right: 5px;
}
