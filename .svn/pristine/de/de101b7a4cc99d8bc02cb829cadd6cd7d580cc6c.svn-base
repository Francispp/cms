/* main_css */
html{
	height:100%
} 
body {
	margin:0px;
	background:#fff;
	font-size:12px;
	text-align:center;
	height:100%;
	font-family: Arial, Helvetica, sans-serif;
}
div{
	display:table;
}
img{
	border:0px;
}
a{
text-decoration:none;
}

#header{
	position: absolute;
	top:0px;
	left:0px;
	width:100%;
	height:84px;
	background:url(../../../images/cms/index1/inside_02.jpg) repeat-x top;
	display:table;
	text-align:right;
}
#header_left{
	width:100%; 
	height:84px;
	background:url(../../../images/cms/index1/inside_01.jpg) no-repeat left top;
}
#header_right{
	width:100%;
	height:84px;
	background:url(../../../images/cms/index1/inside_03.png) no-repeat right top !important;
	background:url();
	filter:none !important;
	filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(enabled=true, sizingMethod=340px, src="../../../images/cms/index1/inside_03.png");
	background:none;
}
#header_nav{
	line-height:30px;
	margin-left:30px;
	margin-right:85px!important;
	margin-right:0px;
	text-align:right !important;
	text-align:left;
	color:#31761A;
	display:block;
}
#header_nav a{
	color:#31761A;
	text-decoration: none; 
	position:relative;
}
#header_nav a:hover{
	color:#CC0000;
	position: relative;
	right: 0px; 
	top: 1px;
	text-decoration: none; 
	position:relative;
}
#header_nav span{
   color:#CC0000;
   }
/* inside mainnav css */
#mainnav_bg{
	position:absolute;
	top:84px;
	left:0px;
	width:100%;
	height:30px;
	background:url(../../../images/cms/index1/inside_07.jpg) repeat-x left top;
}
#mainnav_bg div{
	height:10px;
	overflow:hidden;
}
#mainnav_bg div#menu{
	height:20px;
	overflow:visible;
	float:right;
	width:362px;
}
#mainnav_bg p{
	margin:0px;
	padding:0px;
	float:left;
	text-indent:30px;
	font-size:12px;
	background:url(../../../images/cms/index1/inside_man.gif) no-repeat 16px top;
}
#mainnav_bg div ul{
	clear:left;
	margin:0px;
	padding:0px;
	list-style-type:none;
	display:inline;
}
#mainnav_bg div ul li{
	float:left;
	margin:0px;
	padding:0px;
	text-align:left;
	display:block;
}
#mainnav_bg div ul li span{color:#cc0000;}
#mainnav_bg div ul li a{color:#020101;}
#mainnav_bg div ul li a:hover{color:#CC0000;position: relative; right: 0px; top: 1px;}
#mainnav_bg div ul li#one{width:70px;text-indent:14px;background:url(../../../images/cms/index1/icon01.gif) no-repeat left top;}
#mainnav_bg div ul li#two{width:86px;text-indent:14px;background:url(../../../images/cms/index1/icon02.gif) no-repeat left top;}
#mainnav_bg div ul li#three{width:60px;text-indent:14px;background:url(../../../images/cms/index1/icon03.gif) no-repeat left top;}
#mainnav_bg div ul li#four{width:86px;text-indent:20px;background:url(../../../images/cms/index1/icon04.gif) no-repeat left top;}
#mainnav_bg div ul li#five{width:60px;}
/* inside main css */
#main{
	height:100%;
	width:100%;
}
/* inside main left css */
#main_left{
	float:left;
	width:189px;
	height:100%;
	background:url(../../../images/cms/index1/inside_left_bg.jpg) repeat-y top left;
}
#main_left_split{
	float:right;
	width:6px;
	height:0px !important;
	height:450px;
	overflow:hidden;
	cursor:hand; 
	display:block;
	background:url(../../../images/cms/index1/arrow.gif) no-repeat right center;
}
#main_left_line{
	float:left;
	width:6px;
	height:100%;
	cursor:hand; 
	display:block;
	background: #D1D1D1 url(../../../images/cms/index1/arrow1.gif) no-repeat left center;
}
#main_left_header{
	height:8px;
	width:189px;
	background:url(../../../images/cms/index1/inside_8.jpg) no-repeat left top;
   }
#main_left_center{
	width:189px;
}
#main_left_footer{
	height:100%;
	width:189px;
	background:url(../../../images/cms/index1/inside_10.jpg) no-repeat bottom left;
}
#main_left_center_content{
	width:189px;
	text-align:center;
}
#main_left_center_content ul{
	margin:6px 0px 0px 6px !important;
	margin:0px;
	padding:0px;
	margin-left:0px;
	text-indent:0px;
	text-align:left;
	list-style-type:none;
}
#main_left_center_content ul li{
	margin:0px;
	padding:0px;
	text-indent:30px;
	line-height:32px;
	vertical-align:middle;
	background:url(../../../images/cms/index1/menu_bg2.jpg) no-repeat center top;
	list-style-position:none;
	font-size:14px;
	color:#fff;
	display:block;
	font-weight: bold;
	cursor:hand;
}
#main_left_center_content ul li#change{background:url(../../../images/cms/index1/menu_bg2.jpg) no-repeat center top;}
#main_left_center_content ul li p{margin:0px;padding:0px;overflow:hidden;}
#main_left_center_content ul li a{color:#fff;font-size:14px;font-weight:bold; display:block;}
#main_left_center_content ul li a:hover{background:url(../../../images/cms/index1/menu_bg1.jpg) no-repeat center top;}
#main_right{
	height:100%;
	width:100%;	
}
#main_right_right{
	float:right;
	width:172px; background: #FBFBFB url(../../../images/cms/index1/inside_24.jpg) repeat-y right top;
}
#main_right_header{
	height:1px;
	width:172px;
	background:url(../../../images/cms/index1/bg.jpg) repeat-x top left;
}
#main_right_bottom{
	height:100%;
	width:172px;
	background:url(../../../images/cms/index1/bg.jpg) repeat-y left top;
}
.main_right_center{width:172px;}
.main_right_center div{
	height:6px; 
	overflow:hidden;
}
.main_right_center p{
	margin:0px;
	padding:0px;
	line-height:24px;
	color:#1E7600;
	font-size:14px;
	font-weight:bold;
	text-align:left;
}
.main_right_center p b{
	float:left;
	text-indent:36px;
	background:url(../../../images/cms/index1/inside_20.jpg) no-repeat 16px 5px;
}
.main_right_center p span{
	float:right;
	width:50px;
	text-indent:14px;
	background:url(../../../images/cms/index1/inside_21.jpg) no-repeat left 5px;
}
.main_right_center p span a{
	color:#646464;
	font-size:12px;
	text-decoration:none;
	font-weight:normal;
}
.main_right_center p span a:hover{color:#CC0000;}
.main_right_center ul{
	float:right;
	margin:0px 8px 4px 8px !important;
	margin:0px 4px 4px 8px;
	padding:8px 0px 8px 0px;
	width:152px;
	text-align:left;
	list-style-type:none;
	background:#EDEDED;
}
.main_right_center ul li{
	margin:0px;
	padding:4px 0px 2px 4px !important;
	padding:5px 0px 2px 4px;
	text-align:left;
	text-indent:10px;
	font-size:12px;
	display:block;
	width:148px;
	background:url(../../../images/cms/index1/under_line1.gif) no-repeat left bottom;
}
.main_right_center ul li p span{
	text-indent:0px;
	width:30px;
	background:url();
}
.main_right_center ul li#line{background:url();}
.main_right_center ul li span#other{
	color:#1E7600;
	padding-left:20px !important;
	padding-left:12px;
	font-size:12px;
}
.main_right_center ul li a{
	display:block;
	color:#5F5F5F;
	background:url(../../../images/cms/index1/list-img1.gif) no-repeat left 4px;
}
.main_right_center ul li a:hover{
	color:#cc0000;
}
.main_right_center ul li p{
	margin-top:-5px!important;
	margin-top:0px;
	padding:0px;
	font-weight:normal;
	font-size:12px;
	color:#5F5F5F;
}

.main_right_center ul li p a{
	color:#1E7600;
	text-decoration:underline;
	background:url();
}
.main_right_center ul li p a:hover{
	color:#cc0000;
	position: relative; right: 0px; top: 1px;
}
/* inside main center css */
#main_center{
height:100%;
width:100%;
margin:0px;
padding:0px;
text-align:center;
}
.main_center_title{
width:100%;
height:31px;
background:url(../../../images/cms/index1/titel_bg.jpg) repeat-x top left;
}

.main_center_title_left{
width:100%; 
height:31px;
background:url(../../../images/cms/index1/titel_left.jpg) no-repeat left top;
}
.main_center_title_right{
width:100%;
height:31px;
background:url(../../../images/cms/index1/titel_right.jpg) no-repeat right top;
}
.main_center_title_right_txt{
	float:left;
	margin-top:6px;
	text-indent:20px;
	text-align:left;
	font-size:14px;
	color:#1D7600;
	font-weight: bolder;
	line-height:200%;
}
.main_center_title_left_txt{
	float:right;
	margin-top:10px;
	margin-right:20px;
	text-align:right;
	font-size:12px;
	color:#1D7600;
	font-weight: bolder;
}
.main_center_title_left_txt a{
	color:#1D7600;
	font-weight: bolder;
}
.main_center_title_left_txt a:hover{
	color:#cc0000;
}

#main_center ul{
	margin:0px auto !important;
	margin:0px;
	padding:0px;
	width:85%;
	text-indent:0px;
	list-style-type:none;
}
#main_center ul li{
	margin:0px;
	padding:0px;
	text-align:left;
	margin-top:6px;
	line-height:18px;
	height:18px;
	font-size:12px;
	width:100%;
	color:#1D7600;
	display:table;
	background:url(../../../images/cms/index1/under_line2.gif) repeat-x left bottom;
}
#main_center ul li span{
	float:right;
	color:#61AA48;
}
#main_center ul li b{
	padding-left:10px;
	font-weight:normal;
	background:url(../../../images/cms/index1/inside_23.jpg) no-repeat left 4px;

}
#main_center ul li a{
	color:#000;
	text-decoration:none;
	font-size:12px;
}
#main_center ul li a:hover{color:#f00;}

/* inside footer css */
#footer{
width:100%;
height:26px;
clear:left;
background:url(../../../images/cms/index1/inside_32.jpg) repeat-x left top ;
}
#footer p{
margin:0px;
padding:0px;
height:26px;
line-height:26px;
color:#035DA9;
text-align:left;
}
#footer p span{
float:right; 
height:26px;
line-height:26px;
margin-right:10px;
}
#footer p a{
color:#AF0000;
}
#footer p a:hover{
color:#AF0000;
text-decoration: underline;
}