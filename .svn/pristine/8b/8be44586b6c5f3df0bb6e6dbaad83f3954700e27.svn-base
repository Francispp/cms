
<html>
<head>
<title></title>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<script language="javascript">
var href = window.opener.location.href;
if(href.indexOf("channelId")==-1){
	var chnId = window.opener.document.getElementById("channelId").value;
	if(href.indexOf("?")>0){
		href = href+"&channelId="+chnId;
	}else{
		href = href+"?channelId="+chnId;
	}
}
if(href.indexOf("myTable_p")==-1){
	var pgs = window.opener.document.getElementsByName("myTable_pg");
	if(pgs[0]){
		if(href.indexOf("?")>0){
			href = href+"&myTable_p="+pgs[0].value;
		}else{
			href = href+"?myTable_p="+pgs[0].value;
		}
	}
}
window.opener.location.href=href;
window.close();
</script>
</head>
<body>

</body>
</html>
