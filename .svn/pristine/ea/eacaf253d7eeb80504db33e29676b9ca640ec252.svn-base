/*附件上传标签*/
.attachment_default {
	margin: 0px;
	padding: 0px;
}

.attachment_default div.but_img {
	width: 66px;
	height: 21px;
	margin-bottom: 10px;
	background: #999 url(/images/common/upload.gif) no-repeat left top;
	cursor: hand;
}

.attachment_default div table.attachment_table {
	width: 400px;
}

.attachment_default div table.attachment_table tr td.attachment_name {
	width: 60%;
	text-align: center;
	font-size: 14px;
	height: 30px;
}

.attachment_default div table.attachment_table tr td.attachment_size {
	width: 20%;
	text-align: center;
	font-size: 14px;
}

.attachment_default div table.attachment_table tr td.attachment_delete {
	width: 20%;
	text-align: center;
	font-size: 14px;
}

.attachment_default div table.attachment_table tr.attachment_list td {
	text-indent: 10px;
	font-size: 12px;
	text-align: left;
}

.attachment_default div table.attachment_table tr.attachment_list td a {
	text-decoration: underline;
}

.attachment_default div table.attachment_table tr.attachment_list td.attachment_button
	{
	text-align: center;
}

.attachment_default div table.attachment_table tr.attachment_list td input
	{
	color: #f00;
	cursor: hand;
	background-color: transparent;
	border: 0px;
	text-align: center
}

