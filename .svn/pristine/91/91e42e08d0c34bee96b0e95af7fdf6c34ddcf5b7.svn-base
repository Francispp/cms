BODY {
	TEXT-ALIGN: center; MARGIN: 0px auto; FONT: 12px simsun; 
}
 
.tit {
	BORDER-BOTTOM: #2c6189 1px solid; BORDER-LEFT: #2c6189 1px solid; PADDING-BOTTOM: 3px; PADDING-LEFT: 5px; WIDTH: 948px; PADDING-RIGHT: 5px; BACKGROUND: url(http://c.csdn.net/bbs/t/5/i/pic_titbg02.gif) #347ea1 repeat-x; BORDER-TOP: #2c6189 1px solid; BORDER-RIGHT: #2c6189 1px solid; PADDING-TOP: 3px
}
.tit H1 {
	PADDING-BOTTOM: 0px; MARGIN: 0px; PADDING-LEFT: 0px; PADDING-RIGHT: 0px; COLOR: #fff; FONT-SIZE: 14px; PADDING-TOP: 3px
}
.tit H1 EM {
	MARGIN: 0px 0px 0px 3px; FONT: bold 14px simsun; COLOR: #f00
}
.tit H1 CITE {
	FLOAT: right
}
.tit DFN A {
	FONT-STYLE: normal; TEXT-INDENT: -1000em; MARGIN: -3px 0px 0px 5px; WIDTH: 52px; DISPLAY: block; BACKGROUND: url(http://c.csdn.net/bbs/t/5/i/bbsbtn_fav.gif) no-repeat; FLOAT: right; HEIGHT: 22px; FONT-SIZE: 12px
}
.tit DFN A:visited {
	FONT-STYLE: normal; TEXT-INDENT: -1000em; MARGIN: -3px 0px 0px 5px; WIDTH: 52px; DISPLAY: block; BACKGROUND: url(http://c.csdn.net/bbs/t/5/i/bbsbtn_fav.gif) no-repeat; FLOAT: right; HEIGHT: 22px; FONT-SIZE: 12px
}
.tit CODE A {
	FONT-STYLE: normal; TEXT-INDENT: -1000em; MARGIN: -3px 0px 0px 5px; WIDTH: 52px; DISPLAY: block; BACKGROUND: url(http://c.csdn.net/bbs/t/5/i/bbsbtn_share.gif) no-repeat; FLOAT: right; HEIGHT: 22px; FONT-SIZE: 12px
}
.tit CODE A:visited {
	FONT-STYLE: normal; TEXT-INDENT: -1000em; MARGIN: -3px 0px 0px 5px; WIDTH: 52px; DISPLAY: block; BACKGROUND: url(http://c.csdn.net/bbs/t/5/i/bbsbtn_share.gif) no-repeat; FLOAT: right; HEIGHT: 22px; FONT-SIZE: 12px
}
.fbarb {
	PADDING-BOTTOM: 3px; LINE-HEIGHT: 18px; PADDING-LEFT: 6px; PADDING-RIGHT: 6px; BACKGROUND: #f6f9fe; HEIGHT: 18px; CLEAR: both; BORDER-TOP: #a9cbee 1px solid; PADDING-TOP: 3px
}
.fbarb UL {
	FLOAT: right
}
.fbarb UL.ulleft {
	FLOAT: left
}
.fbarb UL LI {
	FLOAT: left
}
.fbarb UL LI A {
	PADDING-BOTTOM: 0px; PADDING-LEFT: 6px; PADDING-RIGHT: 6px; COLOR: #000; BORDER-RIGHT: #666 1px solid; TEXT-DECORATION: none; PADDING-TOP: 0px
}
.fbarb UL LI A.nob {
	BORDER-RIGHT-WIDTH: 0px; BORDER-TOP-WIDTH: 0px; BORDER-BOTTOM-WIDTH: 0px; BORDER-LEFT-WIDTH: 0px
}
.fbarb UL LI A.dr {
	COLOR: #b00
}
.fbarb UL LI A.gr {
	COLOR: #002d93
}
.fbarb DIV.rt {
	WIDTH: 440px; WHITE-SPACE: nowrap; OVERFLOW: hidden
}
.mtxt {
	PADDING-BOTTOM: 10px; LINE-HEIGHT: 23px; MARGIN: 10px 0px; PADDING-LEFT: 20px; WIDTH: 100%; PADDING-RIGHT: 20px; FONT-SIZE: 14px; PADDING-TOP: 10px
}
.mtxt P.unlog {
	BORDER-BOTTOM: #ccc 1px solid; BORDER-LEFT: #ccc 1px solid; PADDING-BOTTOM: 30px; MARGIN: 30px 0px 0px; PADDING-LEFT: 50px; PADDING-RIGHT: 10px; FONT: bold 14px simsun; BACKGROUND: url(http://c.csdn.net/bbs/t/5/i/pic_09.gif) #efefef no-repeat 10px 20px; BORDER-TOP: #ccc 1px solid; BORDER-RIGHT: #ccc 1px solid; PADDING-TOP: 30px
}
.mtxt P.unlog A {
	PADDING-BOTTOM: 0px; PADDING-LEFT: 5px; PADDING-RIGHT: 5px; COLOR: #c30; PADDING-TOP: 0px
}
.mtxt P.be {
	BORDER-BOTTOM: #ccc 1px solid; TEXT-ALIGN: left; BORDER-LEFT: #ccc 1px solid; PADDING-BOTTOM: 5px; MARGIN: 10px 0px; PADDING-LEFT: 15px; WIDTH: 250px; PADDING-RIGHT: 15px; WHITE-SPACE: nowrap; BACKGROUND: #efefef; COLOR: #b00; FONT-SIZE: 12px; BORDER-TOP: #ccc 1px solid; BORDER-RIGHT: #ccc 1px solid; PADDING-TOP: 5px
}
.msgfont {
	OVERFLOW-X: hidden; WIDTH: 98%; WORD-BREAK: break-all
}
.msgfont SPAN {
	LINE-HEIGHT: 150%
}
.mtxt FIELDSET {
	BORDER-BOTTOM: #cad9ea 1px solid; BORDER-LEFT: #cad9ea 1px solid; PADDING-BOTTOM: 10px; OVERFLOW-X: hidden; MARGIN: 10px; PADDING-LEFT: 10px; WIDTH: 620px; PADDING-RIGHT: 10px; FONT: 12px/19px simsun; WORD-BREAK: break-all; BORDER-TOP: #cad9ea 1px solid; BORDER-RIGHT: #cad9ea 1px solid; PADDING-TOP: 10px
}
.mtxt FIELDSET LEGEND {
	FONT: bold 12px simsun; COLOR: #666
}
.fbart {
	BORDER-BOTTOM: #a9cbee 1px solid; PADDING-BOTTOM: 3px; LINE-HEIGHT: 18px; PADDING-LEFT: 6px; PADDING-RIGHT: 6px; HEIGHT: 18px; PADDING-TOP: 3px
}
.fbart EM {
	PADDING-BOTTOM: 0px; FONT-STYLE: normal; PADDING-LEFT: 3px; PADDING-RIGHT: 3px; FLOAT: right; FONT-WEIGHT: bold; PADDING-TOP: 0px
}
.fbart DFN {
	PADDING-BOTTOM: 0px; FONT-STYLE: normal; PADDING-LEFT: 3px; PADDING-RIGHT: 3px; FLOAT: right; PADDING-TOP: 0px
}
.fbart DFN CODE {
	FONT-STYLE: normal; COLOR: #c30
}
.mframe TD.rb {
	VERTICAL-ALIGN: bottom
}
.mframe {
	TEXT-ALIGN: left; MARGIN: 2px 0px; WIDTH: 100%; FLOAT: left
}
.mframe UL {
	PADDING-BOTTOM: 0px; LIST-STYLE-TYPE: none; MARGIN: 0px; PADDING-LEFT: 0px; PADDING-RIGHT: 0px; PADDING-TOP: 0px
}
.mframe {
	BORDER-BOTTOM: #a9cbee 1px solid; BORDER-LEFT: #a9cbee 1px solid; BORDER-TOP: #a9cbee 1px solid; BORDER-RIGHT: #a9cbee 1px solid
}
.mframe TD.lf {
	PADDING-BOTTOM: 13px; PADDING-LEFT: 15px; WIDTH: 162px; PADDING-RIGHT: 10px; BACKGROUND: #f6f9fe; VERTICAL-ALIGN: top; BORDER-RIGHT: #a9cbee 1px solid; PADDING-TOP: 20px
}
.mframe TD.lf DIV.df {
	WIDTH: 162px
}
.mframe TD.lf UL LI {
	PADDING-BOTTOM: 4px; PADDING-LEFT: 0px; PADDING-RIGHT: 0px; PADDING-TOP: 4px
}
.mframe TD.lf UL LI.center {
	TEXT-ALIGN: left
}
.mframe TD.lf UL LI.center IMG {
	BORDER-BOTTOM: #ccc 1px solid; BORDER-LEFT: #ccc 1px solid; BORDER-TOP: #ccc 1px solid; BORDER-RIGHT: #ccc 1px solid
}
.mframe TD.lf UL LI DFN {
	TEXT-ALIGN: left; PADDING-BOTTOM: 2px; FONT-STYLE: normal; PADDING-LEFT: 0px; PADDING-RIGHT: 0px; DISPLAY: block; FONT-WEIGHT: bold; PADDING-TOP: 2px
}
.mframe TD.lf UL LI DFN A {
	COLOR: #333
}
.mframe TD.lf UL LI CITE {
	LINE-HEIGHT: 25px; FONT-STYLE: normal; MARGIN: 0px 3px 0px 0px; DISPLAY: block; BACKGROUND: url(http://c.csdn.net/bbs/t/5/i/bbspage_addpic4.gif) no-repeat right 50%; FLOAT: left; HEIGHT: 25px
}
.mframe TD.lf UL LI CITE A {
	PADDING-BOTTOM: 0px; PADDING-LEFT: 5px; PADDING-RIGHT: 5px; DISPLAY: block; BACKGROUND: url(http://c.csdn.net/bbs/t/5/i/bbspage_addpic3.gif) no-repeat left 50%; FLOAT: left; PADDING-TOP: 0px
}
.mframe TD.lf UL LI CITE A:hover {
	TEXT-DECORATION: none
}
.mframe TD.lf UL LI EM {
	FONT-STYLE: normal; MARGIN: 23px 0px 0px; FLOAT: left
}
.mframe TD.lf UL LI EM A {
	COLOR: #000
}
.mframe TD.lf UL LI DIV.medalregion {
	FLOAT: left
}
.mframe TD.lf UL LI DIV.medalregion DIV {
	FLOAT: left
}
.mframe TD.lf UL LI DIV.medalregion DIV P.medalnum {
	MARGIN: 2px 0px 0px 19px; FONT: bold 9px Tahoma,simsun; COLOR: #fff
}
.mframe TD.rw {
	VERTICAL-ALIGN: top
}