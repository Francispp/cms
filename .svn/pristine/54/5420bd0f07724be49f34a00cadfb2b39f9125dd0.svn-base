<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.cyberway.weixin.business.attendance.domain.AttendanceManage" table="WX_ATTENDANCE_MANAGE">
        <id name="id" type="java.lang.Long" column="ID">
            <generator class="native" />
        </id>
            <property name="latitude"  type="java.lang.Double" column="LATITUDE" />
            <property name="longitude"  type="java.lang.Double" column="LONGITUDE" />
            <property name="range"  type="java.lang.Integer" column="RANGES" />
    </class>
</hibernate-mapping>