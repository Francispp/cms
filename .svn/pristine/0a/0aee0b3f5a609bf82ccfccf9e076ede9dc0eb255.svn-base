package com.cyberway.cms.domain;

import java.io.Serializable;
import java.util.ArrayList;
import java.util.Date;
import java.util.List;

import com.cyberway.cms.domain.Template;
/**
 * 分发资源类型
 * @author taoz
 *
 */
public class CmsDistribution implements Serializable
{ 
	/**
	 * id
	 */
	private Long id;
	
	/**
	 * 分发种类
	 */
	private String dtype;
	/**
	 * 分发种类名称
	 */
	private String dtypeName;
	/**
	 *最后分发时间 
	 */
	private Date dlastTime;
	/**
	 * 是否选择分发 1为 勾 0为叉
	 */
	private String dipId = "1";
	/**
	 * 站点id
	 */
	private Long siteId;
	/**
     * 备注
	 */
	private String remark;
	public Long getId() {
		return id;
	}
	public void setId(Long id) {
		this.id = id;
	}
	public String getDtype() {
		return dtype;
	}
	public void setDtype(String dtype) {
		this.dtype = dtype;
	}
	public Date getDlastTime() {
		return dlastTime;
	}
	public void setDlastTime(Date dlastTime) {
		this.dlastTime = dlastTime;
	}
	public String getDipId() {
		return dipId;
	}
	public void setDipId(String dipId) {
		this.dipId = dipId;
	}
	public Long getSiteId() {
		return siteId;
	}
	public void setSiteId(Long siteId) {
		this.siteId = siteId;
	}
	public String getRemark() {
		return remark;
	}
	public void setRemark(String remark) {
		this.remark = remark;
	}
	public String getDtypeName() {
		return dtypeName;
	}
	public void setDtypeName(String dtypeName) {
		this.dtypeName = dtypeName;
	}

	
	
}
