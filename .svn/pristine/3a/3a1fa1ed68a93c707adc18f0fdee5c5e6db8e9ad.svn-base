* {
	margin:0;
	padding:0;
}
html, body {
	font-family:Arial, "宋体";
	font-size: 12px;
}
body {
	background:url(../../images/cybercms/bj.gif);
}
ul, li, img {
	list-style-type: none;
	border:none;
}
a {
	text-decoration:none;
	color:#019583;
}
a:hover {
	color:#ff0000;
	text-decoration:none;
}
#leftbar {
	left:0px;
	height:100%;
	width:16px;
	background:url(../../images/cybercms/tree_left.gif) repeat-y;
}
#rightbar {
	left:0px;
	width:16px;
	height:100%;
	background:url(../../images/cybercms/swich1.gif) repeat-y;
}
#conteiner {
	min-width:1260px;
}
.fn-clear {
	clear:both;
}
.fn-left {
	float:left;
}
/*-------------------------------------------------------------baseFrame---------------------------------------------------------------*/
.baseFrame {
	overflow-x:auto;
	overflow-y:hidden;
}
.baseFrame iframe {
	border:none;
}
.baseFrame-ex {
	overflow-x:hidden;
	overflow-y:hidden;
}
#baseFrame-header {
	height:90px;
	overflow:hidden;
}
.baseFrame-content {
	font-size:0;
	zoom:1;
}
.baseFrame-content-left {
	height:100%;
	float:left;
	font-size:0;
	zoom:1;
	border-right:1px solid #a1a1a1;
}
.baseFrame-content-left-item {
	float:left;
}
.baseFrame-content-right {
	_float:left;
	margin-left:271px;
	_margin-left:auto;
	height:100%;
}

#frameLL {width:117px;height:100%;float:left;}
#frameRR {width:153px;height:100%;float:left;}
#LL_show, #RR_show {width:16px;height:100%;float:left;cursor:pointer;background:url(../../images/cybercms/tree_left.gif) repeat-y;}
#LL_close, #RR_close {cursor:pointer;height:14px;background:url(../../images/cybercms/yc2.jpg) repeat-x;}
#LL_close {border:1px solid #a0a0a0;background:url(../../images/cybercms/yc4.gif) repeat-x;height:12px;}
#baseFrame-part1, #baseFrame-part2 {float:left;}
/*-------------------------------------------------------------header---------------------------------------------------------------*/
.header, .header-bj, .header-logo {
	width:100%;
	float:left;
}
.header-bj {
	height:51px;
	background:url(../../images/cybercms/header-bj.gif) repeat-x;
}
.header-logo {
	height:51px;
	background-image: url(../../images/cybercms/logo.gif);
	background-repeat: no-repeat;
}
.header-nav {
	float:right;
	height:51px;
}
.header-nav ul {
	list-style:none;
}
.header-nav ul li {
	text-align:center;
	float:left;
}
.header-nav ul .user {
	height:51px;
	line-height:51px;
	color:#FFFFFF;
	padding:0px 13px;
	float:left;
}
.header-nav ul .user-hover a, .header-nav ul .user-link a, .header-nav ul .user-link a span, .header-nav ul .user-link a:hover, .header-nav ul .user-link a:hover span {
	display:block;
	height:51px;
	line-height:51px;
	padding:0px 12px 0px 14px;
	float:left;
	color:#FFFFFF;
}
.header-nav ul .user-hover a {
	background:url(../../images/cybercms/header-nav.gif) no-repeat;
}
.header-nav ul .user-link a {
	background:url(../../images/cybercms/header-nav.gif) 0px -51px no-repeat;
}
.header-nav ul .user-link a span {
	background:url(../../images/cybercms/header-nav.gif) 0px -51px no-repeat;
}
.header-nav ul .user-link a:hover {
	background:url(../../images/cybercms/header-nav.gif) no-repeat;
}
.header-nav ul .user-link a:hover span {
	background:url(../../images/cybercms/header-nav.gif) no-repeat;
}
.nav-bj {
	width:100%;
	float:left;
	height:39px;
	background:url(../../images/cybercms/nav-bj.gif) repeat-x;
}
.nav-select {
	width:240px;
	height:27px;
	float:left;
	color:#666666;
	text-align:right;
	padding-top:7px;
}
.site-select {
	width:158px;
	height:22px;
	border: 1px solid #999999;
	color:#4782bb;
	margin:0px 10px 0px 8px;
	vertical-align:middle;
}
.nav {
	height:35px;
	float:left;
}
.nav ul {
	list-style:none;
	padding-top:5px;
}
.nav ul li {
	float:left;
	margin-right:2px;
}
.nav ul .nav-hover a, .nav ul .nav-hover a span, .nav ul .nav-link a, .nav ul .nav-link a span, .nav ul .nav-link a:hover, .nav ul .nav-link a:hover span {
	height:30px;
	line-height:30px;
	float:left;
	cursor:hand;
	display:block;
}
.nav ul .nav-hover a {
	padding-left:18px;
	color:#FFFFFF;
	background:url(../../images/cybercms/nav.gif) left top no-repeat;
}
.nav ul .nav-hover a span {
	padding-right:18px;
	color:#FFFFFF;
	background:url(../../images/cybercms/nav.gif) right top no-repeat;
}
.nav ul .nav-link a {
	padding-left:18px;
	color:#666666;
	background:url(../../images/cybercms/nav.gif) left -30px no-repeat;
}
.nav ul .nav-link a span {
	padding-right:18px;
	color:#666666;
	background:url(../../images/cybercms/nav.gif) right -30px no-repeat;
}
.nav ul .nav-link a:hover {
	padding-left:18px;
	color:#0162be;
	background:url(../../images/cybercms/nav.gif) left -30px no-repeat;
}
.nav ul .nav-link a:hover span {
	padding-right:18px;
	color:#0162be;
	background:url(../../images/cybercms/nav.gif) right -30px no-repeat;
}
/*-------------------------------------------------------------menu---------------------------------------------------------------*/

