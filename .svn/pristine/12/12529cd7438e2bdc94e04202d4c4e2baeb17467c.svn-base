<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.cyberway.weixin.business.auth.domain.AuthDetails" table="WX_AUTH_DETAILS">
        <id name="id" type="java.lang.Long" column="ID">
            <generator class="native" />
        </id>
        <property name="agentId" type="java.lang.Integer" column="AGENT_ID"/>
        <property name="agentName" type="java.lang.String" column="AGENT_NAME"/>
        <property name="square_logo_url" type="java.lang.String" column="SQUARE_LOGO_URL"/>
        <property name="round_logo_url"  type="java.lang.String" column="ROUND_LOGO_URL" />
        <property name="appId"  type="java.lang.Integer" column="APP_ID" />
        <property name="createDate"  type="java.util.Date" column="CREATE_DATE" />
    </class>
</hibernate-mapping>