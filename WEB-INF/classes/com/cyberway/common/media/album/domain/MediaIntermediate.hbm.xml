<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.cyberway.common.media.album.domain.MediaIntermediate" table="CMS_MEDIA_INTERMEDIATE">
		<id name="id" type="java.lang.Long" column="id">
			<generator class="native" />
		</id>
		<property name="docId" type="java.lang.Long" />
		<property name="mediaId" type="java.lang.Long" />
		<property name="type" type="java.lang.String" column="MEDIATYPE"/>
		<property name="name" type="java.lang.String" />
	</class>
</hibernate-mapping>