<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="true">
	<class name="com.cyberway.cms.domain.PagePublisherLog" table="CMS_PAGEPUBLISHER_LOG"
		lazy="false">
		<id name="id" type="java.lang.Long">
			<generator class="native" />
		</id>
		<property name="baseURL" length="255"/>
		<property name="ids" length="1000"/>
		<property name="role" />
		<property name="urlKey" />
		<property name="successCount" />
		<property name="failCount" />
		<property name="time" />
	</class>
</hibernate-mapping>
