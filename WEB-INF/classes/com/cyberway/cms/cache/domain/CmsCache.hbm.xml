<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="true">
	<class name="com.cyberway.cms.cache.domain.CmsCache" table="CMS_CACHE" lazy="true">

		<id name="id" type="java.lang.Long">
			<generator class="native" />
		</id>

		<property name="cacheName" not-null="true" />

		<property name="manageClassPath" not-null="true" />

		<property name="initMethodName" not-null="true" />
		
		<property name="getAllMethodName" not-null="true" />
		
		<property name="removeAllMethodName" not-null="true" />

		<property name="getMethodName" not-null="true" />

	</class>
</hibernate-mapping>
