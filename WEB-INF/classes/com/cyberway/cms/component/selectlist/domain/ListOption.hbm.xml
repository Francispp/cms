<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.cyberway.cms.component.selectlist.domain.ListOption" table="CMS_LISTOPTION">

		<id name="oid" column="oid" type="java.lang.Long">
			<generator class="native">
			</generator>
		</id>

		<property name="listTitleId" type="java.lang.Long" column="listTitleId" />

		<property name="key" type="java.lang.String" column="optkey" />

		<property name="value" type="java.lang.String" column="optvalue" />

		<property name="lastUpdateTime" type="java.sql.Timestamp" column="lastUpdateTime" />

		<property name="lastUpdateUser" type="java.lang.String" column="lastUpdateUser" />

		<property name="lastUpdateUserid" type="java.lang.Long" column="lastUpdateUserid" />

		<property name="pos" type="java.lang.Integer" column="pos" />

	</class>

</hibernate-mapping>
