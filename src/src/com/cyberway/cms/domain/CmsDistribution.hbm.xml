<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>

<class 
    name="com.cyberway.cms.domain.CmsDistribution" 
    table="CMS_DISTRIBUTION" 
>
    <id
        name="id"
        type="java.lang.Long"
        column="ID"
    >
        <generator class="native" />
    </id>

    <property
        name="dtype"
        type="java.lang.String"
        column="DTYPE"
        length="200"
    />
    <property
        name="dlastTime"
        type="java.util.Date"
        column="DLASTTIME"
        length="19"
    />
   
    
    <property
        name="remark"
        type="java.lang.String"
        column="REMARK"
        length="2000"
    />
    
    <property
        name="siteId"
        type="java.lang.Long"
        column="SITEID"
        length="2000"
    />
    
</class>
</hibernate-mapping>
