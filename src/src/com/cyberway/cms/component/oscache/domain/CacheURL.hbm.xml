<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.cyberway.cms.component.oscache.domain">
  <class name="CacheURL" table="CMS_CACHEURL">
        <id name="oid" type="java.lang.Long">
            <generator class="native" />
        </id>
        <property name="resource" column="resource1" unique="false" not-null="true" />
        <property name="periods" type="java.lang.Integer"/>
        <property name="cron" />
		<property name="includeurl" />
		<property name="remark" />
		<property name="activated" type="java.lang.Boolean"/>

  </class>
</hibernate-mapping>