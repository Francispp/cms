<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="true">
    <class name="com.cyberway.cms.component.wsr.domain.Subcreibe" table="CMS_SUBCREIBE" lazy="true">
        <id name="id" type="java.lang.Long">
            <generator class="native" />
        </id> 
				<property name="userId"  length="50"/><!--用户编号-->
		<property name="userName" length="200"/><!--用户名-->
		<property name="userEmail" length="100" /><!--邮编-->
		<property name="siteName" length="100"/><!--站点名称-->
		<property name="siteId" length="20"/><!--站点编号-->
		<property name="channelId" length="20"/><!--频道编号-->
		<property name="channelName" length="100"/><!--频道名称-->
		<property name="remoteAddr" length="20"/><!--请求地址-->
		<property name="subcreibeDate" /><!--订阅时间-->
		<property name="lastSubcreibeDate" /><!--最后订阅时间--> 
		<property name="locked" length="10"/><!--订阅状态-->
		<property name="approved" length="10"/><!--审核状态  -->
		    </class>
</hibernate-mapping >
		