<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.cyberway.weixin.business.attendance.domain.FieldRecord" table="WX_FIELDRECORD">
        <id name="id" type="java.lang.Long" column="ID">
            <generator class="native" />
        </id>
            <property name="userId"  type="java.lang.String" column="USER_ID" />
            <property name="userName"  type="java.lang.String" column="USER_NAME" />
            <property name="reason"  type="java.lang.String" column="FIELD_REASON" />
            <property name="signinTime"  type="java.lang.String" column="SIGNIN_TIME" />
            <property name="signOutTime"  type="java.lang.String" column="SIGNOUT_TIME" />
            <property name="state"  type="java.lang.String" column="STATE" />
            <property name="fieldPerson"  type="java.lang.String" column="FIELDPERSON" />
            <property name="startTime"  type="java.lang.String" column="START_TIME" />
            
             <property name="endTime"  type="java.lang.String" column="END_TIME" />
             <property name="fieldPlace"  type="java.lang.String" column="FIELD_PLACE" />
             <property name="fieldType"  type="java.lang.String" column="FIELD_TYPE" />
             <property name="note"  type="java.lang.String" column="NOTE" />
           
	
    </class>
</hibernate-mapping>